{
  // 新版vitepress，否则无法搜索
  "index_name": "cc-index", 
  "start_urls": ["https://cecloudx.github.io/docs/"], 

  "selectors": {
    "lvl0": {
      "selector": "",
      "default_value": "Documentation"
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "text": ".content p, .content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "strip_chars": " .,;:#",
  "selectors_exclude": [".sr-only"],
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"]
  }
}